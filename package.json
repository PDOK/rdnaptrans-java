  "bugs": {
    "url": "https://github.com/reinvantveer/rdnaptrans.js/issues"
  },
  "homepage": "https://github.com/reinvantveer/rdnaptrans.js#readme",
  "dependencies": {
    "co": "^4.6.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
